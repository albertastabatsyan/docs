{
  "name": "@mintlify/common",
  "version": "1.0.24",
  "description": "Commonly shared code within Mintlify",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "type": "module",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc --project tsconfig.build.json",
    "clean:build": "rimraf dist",
    "clean:all": "rimraf node_modules .eslintcache && yarn clean:build",
    "watch": "tsc --watch",
    "test": "vitest run",
    "type": "tsc --noEmit",
    "lint": "eslint . --cache",
    "format": "prettier . --write",
    "format:check": "prettier . --check"
  },
  "author": "Mintlify, Inc.",
  "license": "ISC",
  "devDependencies": {
    "@mintlify/eslint-config": "^1.0.4",
    "@mintlify/eslint-config-typescript": "^1.0.9",
    "@mintlify/ts-config": "^2.0.2",
    "@tsconfig/recommended": "1.x",
    "@types/estree-jsx": "^1.0.0",
    "@types/mdast": "^3.0.10",
    "@typescript-eslint/eslint-plugin": "6.x",
    "@typescript-eslint/parser": "6.x",
    "eslint": "8.x",
    "eslint-config-prettier": "8.x",
    "eslint-plugin-unused-imports": "^3.x",
    "openapi-types": "^12.1.3",
    "prettier": "2.x",
    "rimraf": "^5.0.1",
    "typescript": "^5.2.2",
    "unified": "^10.0.0",
    "vitest": "^0.34.6"
  },
  "dependencies": {
    "@mintlify/models": "0.0.57",
    "@mintlify/validation": "0.1.106",
    "acorn-jsx": "^5.3.2",
    "esast-util-from-js": "^2.0.0",
    "estree-util-to-js": "^2.0.0",
    "gray-matter": "^4.0.3",
    "mdast-util-mdx": "^2.0.0",
    "mdast-util-mdx-jsx": "^2.1.2",
    "remark": "^14.0.2",
    "remark-frontmatter": "^4.0.1",
    "remark-gfm": "^3.0.1",
    "remark-math": "^5.1.1",
    "remark-mdx": "^2.0.0",
    "unist-util-remove": "^3.1.0",
    "unist-util-remove-position": "^4.0.0",
    "unist-util-visit": "^4.1.1"
  },
  "gitHead": "88eb330ccd7c52fbf32859c89a7b5a987f5a4b6e"
}
